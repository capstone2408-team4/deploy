# AWS Configuration
AWS_REGION=us-east-1                 # Region for deployment

# Environment Selection
ENVIRONMENT=production               # 'production' or 'staging'

# Postgres Configuration
POSTGRES_USER=providence             # Postgres admin user
POSTGRES_PASSWORD=                   # Postgres admin password
POSTGRES_DB_NAME=providence          # Postgres database name

# Redis Configuration
REDIS_PASSWORD=                      # Redis password

# Vector Database Configuration
QDRANT_API_KEY=                      # Qdrant API key

# External APIs
OPENAI_API_KEY=                      # OpenAI API key for session analysis
FINDIP_API_KEY=                      # FindIP API key for geolocation

# Providence Configuration
API_PORT=5001                        # API service port
JWT_SECRET=                          # JWT signing key
PROVIDENCE_ROOT_PROJECT=root         # First login username/project name
PROVIDENCE_ROOT_PASSWORD=            # First login password